# Milvus PySpark Demo Configuration
# ===================================
# Copy this file to .env and update the values with your actual credentials

# Milvus Connection Settings
A_MILVUS_URI=http://localhost:19530
A_MILVUS_TOKEN=root:Milvus
A_MILVUS_COLLECTION_NAME=your_collection_name
A_MILVUS_DATABASE_NAME=default

# S3 Configuration (for Milvus storage)
A_S3_FILESYSTEM_TYPE=s3a://
A_S3_ENDPOINT=localhost:9000
A_S3_ACCESS_KEY=minioadmin
A_S3_SECRET_KEY=minioadmin
A_S3_BUCKET_NAME=a-bucket
A_S3_ROOT_PATH=files
A_S3_USE_SSL=false
A_S3_PATH_STYLE_ACCESS=true

# Milvus Connection Settings
B_MILVUS_URI=http://localhost:19530
B_MILVUS_TOKEN=root:Milvus
B_MILVUS_COLLECTION_NAME=your_collection_name
B_MILVUS_DATABASE_NAME=default

# S3 Configuration (for Milvus storage)
B_S3_FILESYSTEM_TYPE=s3a://
B_S3_ENDPOINT=localhost:9000
B_S3_ACCESS_KEY=minioadmin
B_S3_SECRET_KEY=minioadmin
B_S3_BUCKET_NAME=a-bucket
B_S3_ROOT_PATH=files
B_S3_USE_SSL=false
B_S3_PATH_STYLE_ACCESS=true

# Spark Configuration
SPARK_APP_NAME=PySpark Milvus Data Diff
MAVEN_COORDS=com.zilliz:spark-connector_2.13:0.1.12-SNAPSHOT