# Milvus PySpark Demo Configuration
# ===================================
# Copy this file to .env and update the values with your actual credentials

# Milvus Connection Settings
MILVUS_URI=http://localhost:19530
MILVUS_TOKEN=root:Milvus
MILVUS_COLLECTION_NAME=your_collection_name
MILVUS_DATABASE_NAME=default

# S3 Configuration (for Milvus storage)
S3_FILESYSTEM_TYPE=s3a://
S3_ENDPOINT=your_s3_endpoint
S3_ACCESS_KEY=your_s3_access_key
S3_SECRET_KEY=your_s3_secret_key
S3_BUCKET_NAME=your_s3_bucket_name
S3_ROOT_PATH=your_s3_root_path
S3_USE_SSL=true
S3_PATH_STYLE_ACCESS=false

# Spark Configuration
SPARK_APP_NAME=PySpark Milvus Demo
MAVEN_COORDS=com.zilliz:spark-connector_2.13:0.1.12-SNAPSHOT